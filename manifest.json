{
  "manifest_version": 3,
  "name": "Cookies & Local Storage Extractor",
  "description": "Simple extension to extract cookies and local storage of all tabs on broswer",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/pending.png"
  },
  "background": {
    "service_worker": "worker.js",
    "persistant": true
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "cookies",
    "scripting",
    "tabs",
    "webNavigation"
  ]
}